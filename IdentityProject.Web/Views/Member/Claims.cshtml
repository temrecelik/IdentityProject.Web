
@{
    ViewData["Title"] = "Claims";
}
@model List<ClaimViewModel>

<h1>@User.Identity.Name kişisinin cookie'sindeki claim bilgileri</h1>

<table class="table">
    <thead>
        <tr>
            <th>Issuer</th>
            <th>Type</th>
           <th>Value</th>
        </tr>

    </thead>

    <tbody>
        @foreach (var item in Model) { 
        <tr>
            <td>@item.Issuer</td>
            <td>@item.Type</td>
            <td>@item.Value</td>
        </tr>
        }
    </tbody>
   
</table>